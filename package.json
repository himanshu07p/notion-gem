{
  "name": "notion-gemini-assistant",
  "version": "1.0.0",
  "description": "MCP server for Notion integration with Gemini Live",
  "main": "dist/NotionMCPServer.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/NotionMCPServer.js",
    "dev": "ts-node src/NotionMCPServer.ts",
    "mcp": "ts-node src/NotionMCPServer.ts",
    "mcp-http": "ts-node src/MCPHttpServer.ts",
    "watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "notion",
    "mcp",
    "model-context-protocol",
    "gemini",
    "ai",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vercel/node": "^5.3.11",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@notionhq/client": "^2.2.14",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "serverless-http": "^3.2.0"
  }
}
