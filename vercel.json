{
  "version": 2,
  "functions": {
    "api/actions.ts": {
      "memory": 1024
    },
    "api/mcp.ts": {
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/actions",
      "dest": "/api/actions"
    },
    {
      "src": "/webhook",
      "dest": "/api/actions"
    },
    {
      "src": "/webhook/verify",
      "dest": "/api/actions"
    },
    {
      "src": "/auth/callback",
      "dest": "/api/actions"
    },
    {
      "src": "/slash-commands",
      "dest": "/api/actions"
    },
    {
      "src": "/mcp/(.*)",
      "dest": "/api/mcp"
    },
    {
      "src": "/health",
      "dest": "/api/mcp"
    },
    {
      "src": "/ai-interface",
      "dest": "/public/ai-interface.html"
    },
    {
      "src": "/privacy",
      "dest": "/public/privacy.html"
    },
    {
      "src": "/terms",
      "dest": "/public/terms.html"
    },
    {
      "src": "/",
      "dest": "/api/actions"
    }
  ]
}
